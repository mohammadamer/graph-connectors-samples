{
  "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.16/MicrosoftTeams.schema.json",
  "version": "1.0.0",
  "manifestVersion": "1.16",
  "id": "${{TEAMS_APP_ID}}",
  "packageName": "com.package.name",
  "name": {
    "short": "${{APP_NAME}}-${{TEAMSFX_ENV}}",
    "full": "${{APP_NAME}}-${{TEAMSFX_ENV}}"
  },
  "developer": {
    "name": "Contoso",
    "websiteUrl": "https://contoso.com",
    "privacyUrl": "https://contoso.com",
    "termsOfUseUrl": "https://contoso.com"
  },
  "description": {
    "short": "${{APP_NAME}}-${{TEAMSFX_ENV}}",
    "full": "${{APP_NAME}}-${{TEAMSFX_ENV}}"
  },
  "icons": {
    "outline": "outline.png",
    "color": "color.png"
  },
  "accentColor": "#FFFFFF",
  "validDomains": [
    "${{BOT_ENDPOINT}}"
  ],
  "webApplicationInfo": {
    "id": "${{ENTRA_APP_CLIENT_ID}}",
    "resource": "api://${{ENTRA_APP_CLIENT_ID}}"
  },
  "graphConnector": {
    "notificationUrl": "${{BOT_ENDPOINT}}/api/notification"
  }
}
